{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "sysport.xyz"
        }
      ],
      "destination": "https://sysports.de/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "https://forum.ssangyongsports.eu.org"
        }
      ],
      "destination": "https://forum.sysport.org/:path*",
      "permanent": true
    }
  ]
}
